generator client {
  provider = "prisma-client-js"
  output = "../node_modules/.prisma/client"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}


model Characters {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  name String @unique
  height String
  gender String
}
